{
  "name": "banka",
  "version": "1.0.0",
  "description": " ",
  "main": "app.js",
  "scripts": {
    "start:dev": "nodemon app.js --exec babel-node --",
    "start": "babel-node app.js",
    "migrate": "babel-node server/models/createtables.js && npm run seed",
    "unmigrate": "babel-node server/models/undoTable/index.js && npm run migrate && npm run start:dev",
    "test:dev": "  nyc --reporter=html --reporter=text mocha dummy/test --compilers js:babel-core/register --timeout 50000ms --exit",
    "test": "npm run test:dev",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test",
    "seed": "babel-node server/seeder/index.js",
    "build": "babel server -d build",
    "heroku-postbuild": "npm run unmigrate && npm run migrate "
  },
  "author": "olorunwa lawrence",
  "license": "ISC",
  "bugs": {},
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.4",
    "babel-preset-env": "^1.7.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-validator": "^5.3.1",
    "jsonwebtoken": "^8.5.0",
    "mocha": "^5.2.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.8.0",
    "pg-hstore": "^2.3.2",
    "random-string": "^0.2.0",
    "shortid": "^2.2.14",
    "validator": "^10.9.0"
  },
  "devDependencies": {
    "coveralls": "^3.0.2",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.6",
    "nyc": "^13.1.0"
  }
